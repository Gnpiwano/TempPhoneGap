<?xml version="1.0" encoding="utf-8"?>
<widget id="nl.downsider.hybridmaterial1102" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MaterialSeed</name>
    <description>
        Cordova-AngularJS-angularMaterial Seed.
    </description>
    <author email="" href="https://github.com/mario-aleo">
        MarioAleo
    </author>
	
	<feature name="http://api.phonegap.com/1.0/camera"/>
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	
    <content src="index.html" />


    <plugin name="com.logilite.vision.camera" spec="0.2.11" source="pgb" />

    <plugin name="cordova-plugin-geolocation"/>
    <plugin name="com.phonegap.plugins.OrientationLock" src="plugins/com.phonegap.plugins.OrientationLock"/>
    <plugin name="cordova-plugin-device-motion"/>
    <plugin name="cordova-plugin-globalization"/>
    <plugin name="cordova-plugin-statusbar"/>
    <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator"/>
	
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>